#!/usr/bin/env bash

previous_output=""

while true; do
  current_output=$(ps -ax | egrep "ProcessController" | grep -v grep)

  if [[ "$current_output" != "$previous_output" ]]; then
    echo  # Print an empty line to indicate a change
    echo "$current_output"
    previous_output="$current_output"
  fi

  sleep 1
done
